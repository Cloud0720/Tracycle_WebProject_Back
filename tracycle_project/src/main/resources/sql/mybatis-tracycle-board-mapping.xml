<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="BoardMapper">
	
	<resultMap type="userVO" id="selectRM">
		<result property="boardId" column="board_id"/>
		<result property="title" column="title"/>
		<result property="content" column="content"/> 
		<result property="date" column="date"/> 
		<result property="viewCount" column="view_count"/>
		<result property="" column="picture"/>
	</resultMap>

	<!-- column 옆에  Alias 사용... -->
	<sql id="select-board">
		SELECT 
		b.board_id board_id, b.title title, b.content content, b.date date, b.viewCount viewCount, b.picture picture,
		b.user_id user_id, u.nickName nickName, b.area_id area_id, a.area_name area_name, b.category_id category_id,
		c.category_name category_name
	</sql>

	<insert id="writeBoard">

		
		
	</insert>
</mapper>